<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All developments
    </h1>

    <a href="/developments/new" class="btn btn-block btn-outline-secondary">
      Add a new development
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          User
        </th>

        <th>
          Trigger
        </th>

        <th>
          Action
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @developments.each do |development| %>
      <tr>
        <!-- Display development.id start -->
        <td>
          <%= development.id %>
        </td>
        <!-- Display development.id end -->

        <!-- Display development.user_id start -->
        <td>
          <%= development.user_id %>
        </td>
        <!-- Display development.user_id end -->

        <!-- Display development.trigger start -->
        <td>
          <%= development.trigger %>
        </td>
        <!-- Display development.trigger end -->

        <!-- Display development.action_id start -->
        <td>
          <%= development.action_id %>
        </td>
        <!-- Display development.action_id end -->

        <!-- Display development.created_at start -->
        <td>
          <%= time_ago_in_words(development.created_at) %> ago
        </td>
        <!-- Display development.created_at end -->

        <!-- Display development.updated_at start -->
        <td>
          <%= time_ago_in_words(development.updated_at) %> ago
        </td>
        <!-- Display development.updated_at end -->

        <td>
          <a href="/developments/<%= development.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
